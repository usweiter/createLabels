{
  "name": "CreateLabels",
  "productName": "CreateLabels",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win publish never",
    "deploy": "electron-builder --win publish always"
  },
  "build": {
    "productName": "Генерация бирок",
    "target": "NSIS",
    "appId": "CreateLabels",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type":"git",
    "url": "https://github.com/usweiter/createLabels.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.1",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "bmp-js": "^0.1.0",
    "bmp-ts": "^1.0.3",
    "electron-updater": "^4.3.9",
    "fs": "0.0.1-security",
    "html-to-image": "^1.6.2",
    "pica": "^7.0.0",
    "qrcode": "^1.4.4",
    "update-electron-app": "^2.0.1",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  }
}
